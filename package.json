{
  "name": "ytmusic",
  "version": "0.0.1",
  "description": "Youtube music manager.",
  "main": "libr/server.js",
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "start": "npm run webpack && npm run build && node .",
    "webpack": "cd client && npm run build"
  },
  "nodemonConfig": {
    "ext": "html,scss,js,json,ts",
    "exec": "npm run start",
    "ignore": [
      "libr",
      "client/compiled.js",
      "client/compiled.css",
      "Music"
    ]
  },
  "author": "Meow",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/node-fetch": "^2.5.12",
    "@types/sass": "^1.16.0",
    "@types/spotify-web-api-node": "^5.0.3",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "sass": "^1.32.5",
    "spotify-web-api-node": "^5.0.2",
    "typescript": "^4.2.4",
    "youtube-search-without-api-key": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.28",
    "ts-node": "^9.1.1"
  }
}
